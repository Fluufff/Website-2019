<section class="hero-banner hero-banner--home section--shadow">

				<iframe type="text/html" width="720" height="405"
		src="https://www.youtube-nocookie.com/embed/LOfbXMPKTxg?mute=1&autoplay=1&disablekb=1&fs=0&list=PL2xCbtDENJ1bmVUeWmYF1t2QKrYWoTVlO&listType=playlist&loop=1"
